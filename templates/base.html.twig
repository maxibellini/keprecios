  var usuLong = document.getElementById('usu_longitud');
  var xdlat = usuLat.value;
  var xdlong = usuLong.value;
  if (usuid.value != 'x'){
          if (!"geolocation" in navigator) {
            return alert("Tu navegador no soporta el acceso a la ubicaciÃ³n. Intenta con otro");
          }

          const onUbicacionConcedida = ubicacion => {
            //alert("UbicaciÃ³n: "+ubicacion.coords.latitude+' '+ubicacion.coords.longitude);
            usuLat.value=ubicacion.coords.latitude;
            usuLong.value=ubicacion.coords.longitude;
            var urlpos = "{{ path('set_posicion', {'id': 'iddato','latitud': 'latituddato','longitud': 'longituddato'}) }}"; 
            urlpos = urlpos.replace("iddato", usuid.value); 
            urlpos = urlpos.replace("latituddato", usuLat.value); 
            urlpos = urlpos.replace("longituddato", usuLong.value); 
            $.getJSON(urlpos , function(data3) {
             
            });
            if( xdlat == '' && xdlong == '' ){
              location.reload();
            }
          }
          
          const onErrorDeUbicacion = err => {
            //alert("Error: "+err);
          }

          const opcionesDeSolicitud = {
            enableHighAccuracy: true, // Alta precisiÃ³n
            maximumAge: 0, // No queremos cachÃ©
            timeout: 5000 // Esperar solo 5 segundos
          };
          // Solicitar
          navigator.geolocation.getCurrentPosition(onUbicacionConcedida, onErrorDeUbicacion, opcionesDeSolicitud);
  }
};
document.addEventListener("DOMContentLoaded", funcionInit);
